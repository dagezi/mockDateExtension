{
    "manifest_version": 2,
    "name": "MockDateExtention",
    
    "version": "1.0",

    "background" : {
	"scripts" : ["background.js"],
	"persistent": true
    },

    "permissions" : [
	"<all_urls>"
    ],

    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    }
}
